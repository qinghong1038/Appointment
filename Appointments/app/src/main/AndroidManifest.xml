<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.gtaandteam.android.wellcure">

    <application
        android:allowBackup="true"
        android:icon="@drawable/app_icon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/app_icon_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="Wellcure Clinic">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".AppointmentActivity"
            android:label="Appointments"
            android:parentActivityName=".DoctorsActivity" />
        <activity
            android:name=".DoctorsActivity"
            android:label="Doctors" />
        <activity
            android:name=".RegisterActivity"
            android:label="Register" />
        <activity
            android:name=".OTPLoginActivity"
            android:label="Login using OTP"></activity>
        <activity android:name=".ConfirmActivity"></activity>
    </application>

</manifest>